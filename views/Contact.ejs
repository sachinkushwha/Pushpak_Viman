    <%- include('partial/head') %> 
<style>
      body {
        font-family: Arial, sans-serif;
        margin: 0;
        padding: 0;
        box-sizing: border-box;
        background: #f4f4f4;
      }
      header {
        background: #007bff;
        color: white;
        padding: 20px;
        text-align: center;
        font-size: 24px;
      }
      .contact-container {
        max-width: 600px;
        margin: 50px auto;
        padding: 20px;
        background: white;
        border-radius: 10px;
        box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);
      }
      .contact-container h2 {
        text-align: center;
        margin-bottom: 20px;
      }
      .contact-form input,
      .contact-form textarea,
      .contact-form button {
        width: 100%;
        padding: 10px;
        margin: 10px 0;
        border: 1px solid #ccc;
        border-radius: 5px;
      }
      button{
        background-color:#007bff;
        color: white;
      }
      
      .contact-info {
        text-align: center;
        margin-top: 20px;
      }
    </style>
    <div class="contact-container">
      <!-- <h2>Get in Touch</h2> -->
       

      <form class="contact-form" action="/Contact-Us" method="post">
        <input type="text" placeholder="Your Name" required name="Username" />
        <input type="email" placeholder="Your Email" required name="email" />
        <textarea
          rows="5"
          placeholder="Your Message"
          required
          name="Message"
        ></textarea>
        <button type="submit">Send Message</button>
      </form>
      <div class="contact-info">
        <p>Email: support@travelbooking.com</p>
        <p>Phone:+91 7739830496</p>
        <p>Address:Sachin and Ved Travel Agency,Street,Amritsar,India</p>
      </div>
    </div>
  </body>
</html>
